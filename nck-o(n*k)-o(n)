#include<bits/stdc++.h>
using namespace std;
typedef long long lli;
lli nck(lli n,lli k)
{
  lli arr[k+1];
  memset(arr,0,sizeof(arr));
  arr[0]=1;
  for(lli i=1;i<=n;i++)
  for(lli j=min(i,k);j>0;j--)
  arr[j]+=arr[j-1];
  return arr[k];
}
int main()
{
    lli n,k;
    cin>>n>>k;
    cout<<nck(n,k);
    return 0;
}
